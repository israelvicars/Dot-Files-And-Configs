alias c=clear
alias l="ls -AF"
alias ll="ls -lAF"

alias mux=tmuxinator

alias v=vim
alias vim-latest="vim -S ~/.vim/sessions/latest.vim"

alias release-the-kraken="tmuxinator start eflex"
alias vanquish-the-kraken="tmux kill-session -t eflex"

alias egc="ember generate controller"
alias egcp="ember generate component"
alias egm="ember generate model"
alias egr="ember generate route"
alias egt="ember generate template"
